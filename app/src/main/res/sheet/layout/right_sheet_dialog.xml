<?xml version="1.0" encoding="utf-8"?>
<com.stario.launcher.sheet.SheetCoordinator xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/coordinator"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="false">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/design_right_sheet"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical|right"
        android:alpha="0"
        android:fitsSystemWindows="false"
        app:layout_behavior="com.stario.launcher.sheet.behavior.right.RightSheetBehavior" />

</com.stario.launcher.sheet.SheetCoordinator>
